<!-- start main -->
<div id="main" class="clear">
  <!-- start top table options -->
  <div id="topOptions" class="clear">
  </div>
  <!-- end top table options -->
  <div id ="map_<%=@map.id%>_photo" style="position:relative;">
    <%= render partial: "user/maps/partials/posicionamento_das_vagas_no_mapa", locals: {map: @map} %>
  </div>
</div>
<!-- end main -->

<div id="sidebar">
  <div id="head" class="clear">
    <h1><strong><%= @map.codigo%></strong></h1>
    <div id="actions">
    </div>
  </div>
  <div class="sidebox clear" id="vacancy_of_map" map_id="<%=@map.id%>">
    <%= render :partial => 'user/maps/partials/vacancies', :locals => {:map => @map, :available => @available} %>
  </div>
  <%= image_tag "concurso-cefet-2011.jpg", :class => "logo"%>
</div>